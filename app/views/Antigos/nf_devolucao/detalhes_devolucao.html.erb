						<td align="left" class="input" nowrap>&nbsp; </td>							
					</tr>
				
					<tr>
						<td align="right" class="idcampo" nowrap>CNPJ/CPF:</td>
						<td align="left" class="input" nowrap><%= e.CNPJ %></td>
						<td align="right" class="idcampo" nowrap>IE:</td>
						<td align="left" class="input" nowrap><%= e.InscricaoEstadual  %></td>
						<td align="right" class="idcampo" nowrap>CFOP:</td>
						<td align="left" class="input" nowrap><%= e.CFOP  %></td>							
					</tr>						
				<% end %>	
