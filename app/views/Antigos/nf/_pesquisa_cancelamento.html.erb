<%= form_tag nil, :method => "get" do %>
	<% if @pesquisa == [] %><br /><br />
		<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tittab4">
			<tr>
				<td align="center" class="Esconder" id="mensagem" >Não foram encontrados resultados para sua pesquisa.</td>
			</tr>
		</table>
	<% else %>
		<table width="100%" border="0" cellpadding="0" cellspacing="0" class="bgtab0">
			<tr> 
				<td nowrap>	
					<table width="100%" border="0" cellspacing="0" cellpadding="0" class="bgtabZ">
						<tr> 
							<td align="center" class="tittab2" width="3%">&nbsp;</td>
							<td align="center" class="tittab2" width="10%">N° NF</td>
							<td align="center" class="tittab2" width="9%">Série</td>
							<td align="center" class="tittab2" width="10%">Emissão</td>
							<td align="center" class="tittab2" width="10%">Entrega</td>
							<td align="center" class="tittab2" width="11%">Merc.</td>
							<td align="center" class="tittab2" width="8%">IPI</td>
							<td align="center" class="tittab2" width="10%">Enc.</td>
